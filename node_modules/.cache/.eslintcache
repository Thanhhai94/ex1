n        };\n    }\n\n    onDishSelect(dish) {\n      this.setState({\n        selectedDish: dish\n      })\n    }\n    renderDish(dish) {\n      if (dish != null)\n          return(\n              <Card>\n                  <CardImg top src={dish.image} alt={dish.name} />\n                  <CardBody>\n                    <CardTitle>{dish.name}</CardTitle>\n                    <CardText>{dish.description}</CardText>\n                  </CardBody>\n              </Card>\n          );\n      else\n          return(\n              <div></div>\n          );\n  }\n\n    render() {\n        const menu = this.props.dishes.map((dish) => {\n            return (\n              <div key={dish.id} className=\"col-12 col-md-5 m-1\">\n                <Card onClick={() => this.onDishSelect(dish)} >\n                  \n                      <CardImg width='100%' src={dish.image} alt={dish.name} />\n                      <CardImgOverlay>\n                      <CardTitle>{dish.name}</CardTitle>\n                  </CardImgOverlay>\n                  \n                </Card>\n              </div>\n            );\n        });\n\n        return (\n          <div className=\"container\">\n            <div className=\"row\">\n                  {menu}\n            </div>\n            <div className='row'>\n                {this.renderDish(this.state.selectedDish)}\n            </div>\n          </div>\n        );\n    }\n}\n\nexport default Menu;","/Users/haivu/Funix/React/Lab03.2/src/shared/dishes.js",[],[],{"ruleId":"30","severity":1,"message":"31","line":1,"column":8,"nodeType":"32","messageId":"33","endLine":1,"endColumn":12},{"ruleId":"30","severity":1,"message":"34","line":1,"column":17,"nodeType":"32","messageId":"33","endLine":1,"endColumn":26},"no-unused-vars","'logo' is defined but never used.","Identifier","unusedVar","'Component' is defined but never used."]